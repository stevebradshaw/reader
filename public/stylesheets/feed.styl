.jumbotron
  background-color: #bbeecc;

.room-selected
  font-weight: bold;

#messages
  list-style-type: none;
  margin: 0;
  padding: 0;
  float:left;
  width:100%;

#messages li
  padding: 5px 10px;

#messages li:nth-child(odd)
  background: #eee;

.folder
  padding-bottom:2px;
  padding-top:4px;
  font-weight:bold ;
  cursor:pointer;

.feed
  border-left: 3px solid white;
  padding-left: 3px;
  cursor: pointer;

.folder:hover, .feed:hover 
  background-color: #ddeeff;

ul
  padding-left:0px;

li
  list-style-type: none;

.colIco
  position:relative;

.feedSelected
  border-color: #aaaadd;

.entry
  border: 0px;
  border-bottom: 1px ;
  border-style: solid;
  border-color: #dddddd ;

#header
  padding-top: 4px;
  padding-bottom: 4px;

.header:hover
  background-color:#dddddd;

.pubdate
 float:right;

.unread
  font-weight: bold ;

hr
  margin-top:5px ;
  margin-bottom:0px ;
